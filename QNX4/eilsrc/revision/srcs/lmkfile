LC=d:\langs\lc60LCI=$(LC)\includeINC=d:\langs\includeMODEL=sLIBR=$(LC)\$(MODEL).SET: PATH=c:\doscom;d:\util;d:\langs;d:\langs\lc60\bin;$(LC).SET: INCLUDE=$(LC);$(INC);$(LCI).SET: LC=$(LC).SET: LC_OPT=-g0 -m$(MODEL) -D3 -csf.SET: LA_OPT=-i$(LIBR).SET: LINK=.SET: LIB=$(LIBR)AFLAGS=-i$(LC)\$(MODEL)\ .C.OBJ :	$(CC) $*.ASM.OBJ:	lasm $(AFLAGS) $*srcs.exe: mainsrcs.obj datasrcs.obj filesrcs.obj    lc -Lor srcs.exe mainsrcs.obj datasrcs.obj filesrcs.objmainsrcs.obj: inclsrcs.h datasrcs.h globsrcs.h mainsrcs.c   lc $(AFLAGS) mainsrcs.cdatasrcs.obj: datasrcs.c filesrcs.h inclsrcs.h globsrcs.h   lc $(AFLAGS) datasrcs.cfilesrcs.obj: filesrcs.c inclsrcs.h globsrcs.h   lc $(AFLAGS) filesrcs.c