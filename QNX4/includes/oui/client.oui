% oui package for command server clients
% application must provide msg_hdr definition
package client
include msghdr

follow msghdr msghdr_init

opts >C:p
sort >	-C <node> send commands to server on specified node
sort >	-p Playback mode: don't send commands to server

#include "nortlib.h"
init >cic_options(argc, argv, msg_hdr);
